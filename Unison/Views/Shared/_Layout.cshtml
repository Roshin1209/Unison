    <script>
        debugger
         var Role = @Html.Raw(Json.Encode(@Session["Role"]));
        $(document).ready(function () {

            if (Role != "Admin") {
                $('#HomeMenu').hide();
                $('#rpt').hide();
                $('#UserMenu').hide();
                $('#SettingsMenu').hide();
                $('#subMenu_AssetMGMT').hide();
                $('#subMenu_HSP').hide();
                $('#subMenu_Ser').hide();
                $('#subMenu_Manufa').hide();
                $('#subMenu_Assignee').hide();
            }
            if (Role != "Admin" && Role != "User") {
                $('#subMenu_IncidentMGMT2').hide();
                $('#MasterMenu').hide();
            }
           
        });
        function fnLogout() {
            location.href='@Url.Action("Index","Home")'
        }
        function ALERT(MSG) {
            $('#msg').html(MSG);
            $('#Md_AlertBOX').modal('show');
        }
        $(function () {
            setNavigation1();
        });
        function setNavigation() {
            debugger;
            var path = window.location.pathname;
            path = path.replace(/\/$/, "");
            path = decodeURIComponent(path);


            $('.mMenu').each(function () {
                var href = $(this).attr('href');
                if (path.substring(0, href.length) === href) {
                    $(this).closest('a').addClass('active');
                    $(this).closest('ul').addClass('in');
                    $(this).css("baground-color", "#eee");
                }
            });
        }
        function setNavigation1() {
            debugger;
            var path = window.location.pathname;
            path = path.replace(/\/$/, "");
            path = decodeURIComponent(path);

            var view = path.split('/')[1];
            var cont = path.split('/')[0];

            if (path.indexOf('/Home') !=-1) {
                cont = "Home";
            } else if (path.indexOf('/Report') !=-1) {
                cont = "Report";
            } else {
                cont = "Master";
            }

            $('.mMenu').find('li').removeClass('active');
            $('.SubMenu').find('div').removeClass('active');
                if (cont == "Report") {
                    $('#rpt').addClass('active');
                    $('#Charts').addClass('active');
                }
                if (cont == "Home") {
                    $('#Home').addClass('active');
                    $('#HomeMenu').addClass('active');
                }

                if (path.indexOf("/lablePrint") != -1) {
                    $('#rpt').addClass('active');
                    $('#Charts').addClass('active');
                }
            //Managment
                if (path.indexOf("/AssetMgmt")!=-1) {
                    $('#LIMGMT').addClass('active');
                    $('#Tables').addClass('active');
                }
                if (path.indexOf("/IncidentMgmt") != -1) {
                    $('#LIMGMT').addClass('active');
                    $('#Tables').addClass('active');
                }



                if (path.indexOf("/Manufacturer") != -1) {
                    $('#Interface').addClass('active');
                    $('#MasterMenu').addClass('active');
                }
                if (path.indexOf("/Requestor") != -1) {
                    $('#Interface').addClass('active');
                    $('#MasterMenu').addClass('active');
                }
                if (path.indexOf("/ServiceProvider") != -1) {
                    $('#Interface').addClass('active');
                    $('#MasterMenu').addClass('active');
                }

                if (path.indexOf("/HospitalList") != -1) {
                    $('#Interface').addClass('active');
                    $('#MasterMenu').addClass('active');
                }
                if (path.indexOf("/Assignee") != -1) {
                    $('#Interface').addClass('active');
                    $('#MasterMenu').addClass('active');
                }
            //Settings
                if (path.indexOf("/ProblemCategory") != -1) {
                    $('#SettingsMenu').addClass('active');
                    $('#Appviews').addClass('active');
                }
                if (path.indexOf("/StatusMaster") != -1) {
                    $('#SettingsMenu').addClass('active');
                    $('#Appviews').addClass('active');
                }
                if (path.indexOf("/RootCauseMaster") != -1) {
                    $('#SettingsMenu').addClass('active');
                    $('#Appviews').addClass('active');
                }
                if (path.indexOf("/EmailSettingMaster") != -1) {
                    $('#SettingsMenu').addClass('active');
                    $('#Appviews').addClass('active');
                }
            //Setting Menu Done.

               //User Menu
                if (path.indexOf("User") != -1) {
                    $('#UserMenu').addClass('active');
                    $('#Forms').addClass('active');
                }


        }
    </script>

